[run]
source = .
branch = True
relative_files = True
omit = 
    test/*
    */__pycache__/*
    */site-packages/*
    setup.py
    conftest.py
    .github/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
